<div class="show_heading">Listing documents</div>

<table class="show_listing">
  <tr>
    <th>Document Sought</th>
    <th>Status</th>
    <th></th>
    <th></th>
  </tr>

<% @documents.each do |document| %>
  <tr>
    <td><%=h document.name %></td>
    <td><%=h document.status %></td>
    <td><%= link_to '<img src="/images/edit.gif" title="Edit"/>', edit_document_path(document) %></td>
  </tr>
<% end %>
</table><br />


